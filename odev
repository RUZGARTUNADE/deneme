meme_dict = {
            "CRINGE": "Garip ya da utandırıcı bir şey",
            "LOL": "Komik bir şeye verilen cevap",
            "FPS": "Her saniye başına düşen kare sayısı",
            "OPSİYONEL":"Herkes için farklı bir anlama gelen ",
            }
            
print(meme_dict)
word = input("Anlamadığınız bir kelime yazın (hepsini büyük harflerle yazın!): ")


if word in meme_dict.keys():
    print("istediğin kelime yukarıda var")
else:
    print("bunu bende bilmiyorum")
